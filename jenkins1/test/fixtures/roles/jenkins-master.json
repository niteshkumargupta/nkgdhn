{
  "name": "jenkins-master",
  "description": "Test install & configure master Jenkins node",
  "json_class": "Chef::Role",
  "default_attributes": {},
  "override_attributes": {
    "jenkins": {
      "master": {
        "listen_address": "127.0.0.1"
      }
    },
    "didata-jenkins": {
      "loglevel": "debug"
    },
    "apt": {
      "compile_time_update": true
    }
  },
  "chef_type": "role",
  "run_list": [
    "recipe[didata-jenkins::master]",
    "recipe[didata-jenkins::credentials]"
  ],
  "env_run_lists": {}
}
